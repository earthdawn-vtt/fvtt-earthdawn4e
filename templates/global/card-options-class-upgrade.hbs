<div class="itemControlGroup" data-item-id="{{_id}}">
  {{#if @root.editable}}
    <a class="item-control item-upgrade__class" data-action="upgradeItem" title="{{localize 'ED.Controls.upgrade'}}">
      <i class="fas fa-arrow-circle-up"></i>
    </a>
  {{/if}}
  {{!-- <a class="item-control item-downgrade" data-action="displayChild" title="{{localize 'ED.Controls.downgrade'}}">
    <i class="fas fa-arrow-circle-down"></i>
  </a> --}}
  <a class="item-control item-display" data-action="displayChild" title="{{localize "ED.Controls.displayItem"}}">
    <i class="fas fa-comments"></i>
  </a>
  <a class="item-control item-delete" data-action="deleteChild" data-uuid="{{uuid}}" title="{{localize "ED.Controls.delete"}}">
    <i class="fas fa-trash"></i>
  </a>
  <a class="item-control item-edit" data-action="editChild" data-uuid="{{uuid}}" title="{{localize "ED.Controls.edit"}}">
    <i class="fas fa-edit"></i>
  </a>
</div>