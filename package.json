    "type": "git",
    "url": "https://github.com/earthdawn-vtt/fvtt-earthdawn4e"
  },
  "bugs": {
    "url": "https://github.com/earthdawn-vtt/fvtt-earthdawn4e/issues"
  },
  "scripts": {
    "build": "npm run build:packs && npm run build:css",
    "build:css": "gulp buildCSS",
