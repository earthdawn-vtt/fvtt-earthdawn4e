    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@foundryvtt/foundryvtt-cli": "^1.0.3",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@stylistic/eslint-plugin-migrate": "^2.3.0",
    "cspell": "^9.0.1",
    "dpdm": "^3.14.0",
    "eslint": "^9.6.0",
    "eslint-plugin-jsdoc": "^46.8.2",
